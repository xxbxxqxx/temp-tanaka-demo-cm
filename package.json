  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "htmlhint": "globstar -- htmlhint \"./src/[^stories|^_scss]*/**/*.html\" --format compact > ./test/htmlhint.log",
    "stylelint": "globstar -- stylelint \"./src/[^stories|^_scss]*/**/{*.css,*.scss}\" --formatter compact > ./test/stylelint.log",
    "eslint": "globstar -- eslint \"./src/{components,pages,templates}*/**/{*.js,*.ts}\" --no-error-on-unmatched-pattern=true --format compact --output-file ./test/eslint.log",
    "eslint_warning": "globstar -- eslint \"./src/{components,pages,templates}*/**/{*.js,*.ts}\" --no-error-on-unmatched-pattern=true --debug --format compact --output-file ./test/eslint_warning.log; exit 0",
    "prettier": "globstar -- prettier --write \"{components,pages,templates}*/**/{*.js,*.ts}\"",
    "format:js": "npm-run-all -s prettier eslint",
    "format:ts": "npm-run-all -s prettier_ts eslint_ts",
    "esdoc": "esdoc -c .esdoc.json"
  },
